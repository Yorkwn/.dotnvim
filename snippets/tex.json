{
	"Equation": {
		"prefix": "eq",
		"body": [
			"\\begin{equation}",
			"\t$0",
			"\\end{equation}"
		],
		"description": "equation-template"
	},

	"Align": {
		"prefix": "al",
		"body": [
			"\\begin{align}",
			"\t$0",
			"\\end{align}"
		]
	},

	"Equation*": {
		"prefix": "eqs",
		"body": [
			"\\begin{equation*}",
			"\t$0",
			"\\end{equation*}"
		],
		"description": "equation*-template"
	},

	"Figure": {
		"prefix": "fig",
		"body": [
			"\\begin{figure}[$1]",
			"\t\\centering",
			"\t\\includegraphics[$3]{$2}",
			"\t\\caption{$4}",
			"\\end{figure}",
			"$0"
		]
	},

	"Engrish": {
		"prefix": ["en", "εν"],
		"body": "\\engl{$1} $0"
	},

